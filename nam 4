using System;
using System.Linq;

namespace ConsoleApp1
{
    class Program
    {
        static void Main()
        {
            Console.Write("n-ое кол-во часов ");
            int n = int.Parse(Console.ReadLine());
            int a = 0, b = 0;

            for (int i= 0; i <= n; i++)
            {
                a = 12 + i; b = i * 5 + 5;
               
            }
            Console.WriteLine(a + ":"  + b);
        }
    }
}
