using System;
using System.Linq;


namespace ConsoleApp6
{




    class Program
    {
        class Tree
        {
            public string Wood { get; set; }
            public int Number { get; set; }

            public Tree(string wood, int number)
            {
                Wood = wood;
                Number = number;

            }
          
            public void Print() { Console.WriteLine($"Деревья {Wood}  и кол-во на складе  {Number}"); }
        }

        static void Main()
        {

            var tree1 = new Tree("сосна", -6);
            var tree2 = new Tree("береза", 80);
             tree1.Print();
             tree2.Print();

        }

    
    }
}
