using System;


namespace ProgLang
{
    class ConsoleApp4
    {
        public static void Print(string simv,int amount)
        {
            for(int i = 0; i < amount; i++)
            {
                Console.Write(simv + " ");
            }
        }
        static void Main()
        {
            Console.WriteLine("символы");
            string simv=Console.ReadLine();
            Console.WriteLine("кол-во");
            int amount = int.Parse(Console.ReadLine());
            Print(simv, amount);
        }
    }
}
